{{ 'main-page.css' | asset_url | stylesheet_tag }}

<div class="boxed">
  <div class="grid">
    <div class="grid__item">
      <div class="section-header">
        <h1>{{ page.title }}</h1>
      </div>

      <div class="rte">
        {{ page.content }}
      </div>
    </div>
  </div>
</div>

{% schema %}
  {
    "name": "Page",
    "presets": [
        {
            "name": "Page"
        }
    ]
  }
{% endschema %}
