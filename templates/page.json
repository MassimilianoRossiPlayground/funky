    "bc968b9e-f027-4344-9c59-ccdcc3406452": {
      "type": "rich-text",
      "settings": {
        "full_width": false,
        "section_title": "Anni 80, Como, un gruppo di ragazzi con una passione incredibile per gli sport estremi, un laboratorio, uno stereo e una musicassetta piena di tracce funky.",
